{
  "name": "TorrServer",
  "version": "1.3.4",
  "slug": "TorrServer",
  "legacy": false,
  "maintenance": {
    "github_release": "https://github.com/YouROK/TorrServer/",
    "include_prerelease": true,
    "version_regex": "(\\d+\\.\\d+\\.?\\d*-?\\w*.?\\d*)"
  },
  "description": "Torrent Server",
  "url": "https://github.com/redacid/hassio-addons/tree/main/ra-TorrServer#readme",
  "startup": "services",
  "arch": [
    "arm64",
    "amd64",
    "arm/v7"
  ],
  "ports": {
    "8090/tcp": 8090
  },
  "ports_description": {
    "8090/tcp": "web interface"
  },
  "map": [
    "config:rw",
    "share:rw",
    "ssl"
  ],
  "webui": "http://[HOST]:[PORT:8090]",
  "boot": "auto",
  "options": {},
  "schema": {},
  "image": "ksey/torrserver:latest"
}
